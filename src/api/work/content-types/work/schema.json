      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "front_vertical": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "front_square": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "data": {
      "type": "richtext",
      "required": false
    },
    "websiteLink": {
      "type": "string",
      "regex": "\\bhttps?:\\/\\/[^\\s/$.?#].[^\\s]*\\b"
    },
    "slug": {
      "type": "string"
    },
    "clientType": {
      "type": "enumeration",
      "enum": [
        "a",
        "b",
        "c"
      ],
      "required": true
    },
    "workType": {
      "type": "enumeration",
      "enum": [
        "a",
        "b",
        "c"
      ],
      "required": true
    },
    "detailsData": {
      "displayName": "detailsData",
      "type": "component",
      "repeatable": true,
